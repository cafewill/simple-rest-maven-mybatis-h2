<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
  <properties resource="mybatis/generator.properties"/>

  <context id="read" targetRuntime="MyBatis3Simple">
    <property name="javaFileEncoding" value="UTF-8"/>
    <property name="autoDelimitKeywords" value="true"/>
    <property name="beginningDelimiter" value="`"/>
    <property name="endingDelimiter" value="`"/>

    <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />
    <plugin type="com.softwareloop.mybatis.generator.plugins.LombokPlugin">
        <property name="builder" value="true"/>
        <property name="accessors" value="true"/>
    </plugin>
    
    <!-- 
	<commentGenerator type="com.cube.simple.util.CustomCommentGenerator">
	    <property name="addRemarkComments" value="true"/>
	</commentGenerator>
     -->
    <commentGenerator>
        <property name="suppressDate" value="true"/>
        <property name="addRemarkComments" value="true"/>
    </commentGenerator>
        
    <jdbcConnection driverClass="${jdbc.driverClass}" connectionURL="${jdbc.url}" userId="${jdbc.user}" password="${jdbc.password}"/>
    <javaTypeResolver><property name="useJSR310Types" value="true"/></javaTypeResolver>
    <javaModelGenerator targetPackage="com.cube.simple.model" targetProject="${gen.java.dir}"><property name="trimStrings" value="true"/></javaModelGenerator>
    <sqlMapGenerator targetPackage="mapper/read" targetProject="${gen.resources.dir}"/>
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.cube.simple.mapper.read" targetProject="${gen.java.dir}"/>

    <table schema="jejuyeora" tableName="bookmarks_restaurant" mapperName="ReadBookmarksRestaurantMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="likes_restaurant" mapperName="ReadLikesRestaurantMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="region" mapperName="ReadRegionMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="residence" mapperName="ReadResidenceMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_images" mapperName="ReadRestaurantImagesMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_menus" mapperName="ReadRestaurantMenusMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_reservations" mapperName="ReadRestaurantReservationsMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_reviews" mapperName="ReadRestaurantReviewsMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_waitings" mapperName="ReadRestaurantWaitingsMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurants" mapperName="ReadRestaurantsMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurants_categories" mapperName="ReadRestaurantsCategoriesMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="users" mapperName="ReadUsersMapper" enableInsert="false" enableUpdateByPrimaryKey="false" enableDeleteByPrimaryKey="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
  </context>

  <context id="write" targetRuntime="MyBatis3">
    <property name="javaFileEncoding" value="UTF-8"/>
    <property name="autoDelimitKeywords" value="true"/>
    <property name="beginningDelimiter" value="`"/>
    <property name="endingDelimiter" value="`"/>

    <plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />
    <plugin type="com.softwareloop.mybatis.generator.plugins.LombokPlugin">
        <property name="builder" value="true"/>
        <property name="accessors" value="true"/>
    </plugin>
    
    <!-- 
	<commentGenerator type="com.cube.simple.util.CustomCommentGenerator">
	    <property name="addRemarkComments" value="true"/>
	</commentGenerator>
     -->
    <commentGenerator>
        <property name="suppressDate" value="true"/>
        <property name="addRemarkComments" value="true"/>
    </commentGenerator>
    
    <jdbcConnection driverClass="${jdbc.driverClass}" connectionURL="${jdbc.url}" userId="${jdbc.user}" password="${jdbc.password}"/>
    <javaTypeResolver><property name="useJSR310Types" value="true"/></javaTypeResolver>
    <javaModelGenerator targetPackage="com.cube.simple.model" targetProject="${gen.java.dir}"/>
    <sqlMapGenerator targetPackage="mapper/write" targetProject="${gen.resources.dir}"/>
    <javaClientGenerator type="XMLMAPPER" targetPackage="com.cube.simple.mapper.write" targetProject="${gen.java.dir}"/>

    <table schema="jejuyeora" tableName="bookmarks_restaurant" mapperName="WriteBookmarksRestaurantMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="likes_restaurant" mapperName="WriteLikesRestaurantMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="region" mapperName="WriteRegionMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="residence" mapperName="WriteResidenceMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_images" mapperName="WriteRestaurantImagesMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_menus" mapperName="WriteRestaurantMenusMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_reservations" mapperName="WriteRestaurantReservationsMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_reviews" mapperName="WriteRestaurantReviewsMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurant_waitings" mapperName="WriteRestaurantWaitingsMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurants" mapperName="WriteRestaurantsMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="restaurants_categories" mapperName="WriteRestaurantsCategoriesMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
    <table schema="jejuyeora" tableName="users" mapperName="WriteUsersMapper" enableSelectByPrimaryKey="false" enableSelectByExample="false"> <property name="useActualColumnNames" value="false"/> <columnRenamingRule searchString="^is_" replaceString=""/> </table>
  </context>
</generatorConfiguration>